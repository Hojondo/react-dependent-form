{
  "include": ["src/**/*"],
  "exclude": "node_modules/**",
  "presets": [
    "@babel/react",
    [
      "@babel/env",
      {
        "modules": false
      }
    ],
    "@babel/typescript"
  ],
  "plugins": [
    "@babel/plugin-proposal-nullish-coalescing-operator",
    "@babel/plugin-proposal-optional-chaining"
  ]
}
